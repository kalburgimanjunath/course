.areaView .viewmode .checkbox {
    cursor: no-drop;
    color: #e1e1e1;
    pointer-events: none;
}

.areaView .viewmode input[type="checkbox"] {
    opacity: .4 !important;
    pointer-events: none;
}

.areaView .viewmode .btn-footer {
    display: none;
}

.addArea .btn-footer {
    margin-top: 10px;
    padding-top: 10px;
    left: -16px;
    background-color: #fff;
    border-color: #fff;
}


@media (max-width: 1366px) and (min-width: 1441px) {

    .addArea .listOverview .sensorsTab, .listOverview .settingsTab {
        max-height: 400px;
    }
}

@media screen and (min-width: 1920px) {

    .addArea .listOverview .sensorsTab, .listOverview .settingsTab {
        max-height: 804px;
    }
}

@media screen\0 {

    .addArea .ispicon.inverse::before {
        width: 20px;
        left: 24px;
        top: 43px;
    }
}

/* AREA SETTINGS */
.areaSection .areaContent > p {
    margin: 20px 10px 20px 15px;
}

.areaSection .link > .add {
    font-size: 60px;
}

.areaSection .link {
    color: #1792e5;
}

.areaSection .areaBox {
    padding: 70px;
    height: 200px;
    text-align: center;
    cursor: pointer;
}

.areaSection .contentContainer {
    border-color: #e7e7e7;
    margin: 10px 0;
    padding: 10px;
    background: #fff;
}

/*.ispheaderbar {
    position: fixed;
    left: 0;
    right: 0;
    z-index: 1;
}*/

.areaSection .form-group {
    padding-left: 0;
}



.areaSection .checkbox input[type="checkbox"], .areaSection .checkbox input[type="radio"] {
    opacity: 1;
    z-index: 0;
}

.areaSection .checkbox label::before {
    border: 0;
}

.areaView .areaOverview .checkbox label::before, .areaView .checkbox label::before {
    display: none;
}

.infoModal {
    margin: 0 auto;
    top: 10% !important;
    left: 0 !important;
    text-align: center;
    width: 400px;
    height: 175px;
}

    .infoModal .modal-content {
        width: 400px;
        padding: 30px;
    }

    .infoModal .modal-body > button {
        margin-top: 30px;
    }


.accountcontainer .overview .content {
    background: 0 !important;
    box-shadow: none !important;
}

/*.viewmode input [type="checkbox"] {
    background: 0;
}*/

.areaView .settingsTab .form-group {
    padding-left: 0;
}

/* NEW ADDITION 24-08-2016*/
.areaView .btn-group .btn + .btn {
    margin-left: 8px !important;
}

.areaView .btn-footer {
    position: absolute;
    bottom: 0;
    right: -15px;
    border-top: 1px solid #ccc;
    left: 0;
    padding: 10px 30px 10px 10px;
}

.areaOverview {
    position: absolute;
    left: 33%;
    bottom: 0;
    top: 0px;
    padding: 0 15px 0 0;
}

    .areaOverview .tab-content .contentContainer {
        position: absolute;
        right: -15px;
        bottom: 53px;
        top: 101px;
        left: 10px;
        overflow-y: auto;
    }

    .areaOverview .tab-content .sensorSearch.show + .sensorsTab .contentContainer {
        top: 180px !important;
        left: 0;
    }

    .areaOverview .tab-content .settingsTab .contentContainer {
        padding-top: 15px;
    }

    .areaOverview .tab-content .viewmode .contentContainer {
        bottom: 0;
    }

    .areaOverview .overviewHeader {
        padding-left: 20px;
    }

.areaView .listOverview .isppagetitle {
    margin-left: 0;
    margin-right: 20px;
}

.areaOptions .checkbox {
    clear: both;
    margin-bottom: 15px;
}

.sensorSearch {
    margin: 0 5px 0 20px;
    border-bottom: 1px solid #e7e7e7;
    padding-bottom: 14px;
    padding-left: 0;
    font-size: 16px;
    padding-top: 14px;
    height: 63px;
}

    .sensorSearch span {
        margin-top: 11px;
    }

    .sensorSearch .btn {
        margin-top: 1px;
    }

    .sensorSearch .searchInput {
        margin: 0 14px;
        float: left;
        padding: 0;
        float: right;
    }

    .sensorSearch .alarm-searchIcon {
        height: 30px;
        width: 20px;
        z-index: 1000;
        position: absolute;
        float: right;
        right: 17px;
        top: 6px;
        margin-top: 0;
    }

