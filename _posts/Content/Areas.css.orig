#area-view
{
  position:absolute;
  left:24%;
  top: 0px;
  bottom: 0px;
  right: 0px;
}

/*for plugin container that needed AccordionPlugin menu after toggle menu*/
.togglemenu #area-view
{
  position:absolute;
  left:70px;
  top: 0px;
  bottom: 0px;
  right: 0px;
}


.selectController {
    width: 100%;
    padding: 15px 20px;
    border-top: 1px solid #e7e7e7;
}

.areaListRow {
    border-radius: 0;
}

.areaView .ispheadersearch .ispicon_sort {
    border: 0 none;
    margin-right: 10px;
}

.areaView .ispheadersearch .ispsearch {
    color: #303030;
}

.cardlistview .listcount {
    background: #f2f2f2;
    margin-right: 0;
    padding: 4px 20px !important;
}

.areaView .cardlistview .list-group.scroll-x.scroll-y {
    overflow-y: auto;
    position: absolute;
    bottom: 0;
    top: 201px;
    right: 0;
    left: 0;
    margin-bottom: 0;
    border-top: 1px solid #e7e7e7;
}

    .areaView .cardlistview .list-group.scroll-x.scroll-y li:first-child {
        border-top: 0 none;
    }

.areaView .cardlistview {
    width: 33%;
    float: left;
    position: absolute;
    left:0;
    bottom: 0;
    top: 0;
}

.areaView  .listOverview {
    border-left: 1px solid #e7e7e7;
}

    .areaView .listOverview .headerTitle {
        font-family: 'HoneywellSansWeb-Bold' !important;
        color: #303030;
        margin-left: 20px;
    }

.areaView .sensorsTab .table > thead > tr > th, .addArea .table > thead > tr > th {
    border-bottom: 0 none;
}

.addArea table td {
    border-top: 1px solid #e7e7e7 !important;
}

.areaView .sensorsTab span.name {
    font-family: 'HoneywellSansWeb-Bold' !important;
    color: #1792e5;
    font-size: 14px;
}

.areaView .sensorsTab span.sensorsid {
    font-family: 'HoneywellSansWeb-Medium' !important;
    color: #606060;
    font-size: 12px;
    text-transform: uppercase;
}

.areaView .sensorsTab table th {
    font-family: 'HoneywellSansWeb-Bold' !important;
    font-size: 14px;
}

.areaView .sensorsTab table td, .areaView .sensorsTab table td span {
    font-family: 'HoneywellSansWeb-Medium' !important;
    font-size: 14px;
}

.areaView .sensorsTab .ispicon, .addArea .ispicon {
    margin-top: 10px;
}

.areaView .sensorsTab .name, .addArea .name {
    margin-left: 10px;
    float:left;
    margin-top:10px;
}

.areaView .sensorsTab .sensorsid, .addArea .sensorsid {
    margin-left: 0 !important;
}

.areaView .sensorsTab .ispicon_collapse {
    margin-left: 15px;
}

.areaView .table td {
    border-top: 1px solid #f0f0f0 !important;
}

.areaView .cardlistview {
    padding: 0px;
    /*margin-left: -15px;*/
    /*border-right: 1px solid #e7e7e7;*/
}

.areaView .cardlistview .isppagetitle, .areaView .listOverview .isppagetitle {
    font-family: 'HoneywellSansWeb-Bold' !important;
    color: #303030;
    margin-left: 20px;
    margin-right: 12px;
}

.areaView .listOverview .isppagetitle {
    margin-left: 9px;
    margin-right: 24px;
}

.areaView .cardlistview .nav.nav-tabs {
    margin-left: 20px;
}

.areaView .list-group-item {
    padding: 12px 20px 10px 20px;
}

.areaView .sensorsList > span + span {
    margin-left: 5px;
}

.areaView .checkbox {
    float: left;
}

    .areaView .checkbox input[type="checkbox"], .addArea .checkbox input[type="checkbox"] {
        opacity: 1 !important;
        margin-top: 0;
    }

.addArea .modal-dialog, .addDevice .modal-dialog {
    width: 700px;
    padding-top: 10px;
}

.addArea.ispmodal, .addDevice.ispmodal {
    left: 0 !important;
}

.addArea .listHeader {
    font-family: 'HoneywellSansWeb-Bold' !important;
    color: #303030;
    border-top: 1px solid #e7e7e7;
    border-bottom: 1px solid #e7e7e7;
    padding: 15px 0;
    margin: 0 15px 10px 15px;
    font-size: 18px;
    clear: both;
}

    .addArea .listHeader + hr {
        margin-bottom: 0;
        margin-top: 10px;
    }

.addArea .panel-body, .addDevice .panel-body {
    padding: 5px 15px 0 15px;
}

.addArea .addDeviceIcon {
    margin-left: 10px;
}

.addArea .chimeZone {
    margin-top: 32px;
}

    .addArea .chimeZone > span {
        padding-top: 3px;
        margin-left: 10px;
        float: left;
    }

.addArea .btn-group button.moveArea {
    margin-left: 10px !important;
}

/*.viewmode input[type="text"], .viewmode textarea, .viewmode select {
    font-family: 'HoneywellSansWeb-Medium' !important;
    font-size: 14px;
    color: #e1e1e1;
    pointer-events: none;
    border: 1px solid #e1e1e1;
    cursor: no-drop;
}

.viewmode .ispicon {
    pointer-events: none;
    opacity: .4;
}

.viewmode .link {
    color: #e1e1e1;
}

.viewmode .form-group label {
    color: #e1e1e1;
    cursor: default;
    pointer-events: none;
    cursor: no-drop;
